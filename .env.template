# .env template
# Copy this file to .env and fill in your values

# Node Environment
NODE_ENV=development
PORT=3000

# Anthropic Claude API
CLAUDE_API_KEY=sk-ant-<your_claude_api_key>

# AWS DynamoDB
AWS_REGION=us-east-1
DYNAMO_TABLE=mpai-sessions

# Optional: Additional configuration
DEBUG=false

# AWS credentials (placeholders, not real keys)
AWS_ACCESS_KEY_ID=REPLACE_WITH_YOUR_AWS_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=REPLACE_WITH_YOUR_AWS_SECRET_KEY
